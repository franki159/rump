                    <label>Tipo</label>
                    <select id="cmbTIPO" class="form-control" onchange="mTipoSelect(this)"></select>
                </div>
                <div class="form-group ">
                    <label>RAZA</label>
                    <select id="cmbRAZA" class="form-control" onchange="mRazaSelect(this)"></select>
