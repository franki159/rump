                    <img src="~/Areas/WebRecognition/img/huella2.png" id="imgTomada" style="width:250px; height:250px;" crossorigin="anonymous" />
                </div>
            </div>
        </div>



        <div class="col-sm-4">
            <div style="text-align:center;margin:0 auto; width:99%;">
                <button id="btnProcessRecognition" class="btn-lg btn-info"><span class="glyphicon-cloud"></span> Process Recognition</button>

                <div class="form-group " style="width:100%;">
                    <div class="table-responsive" style="width:100%;">
                        <table id="tbResult" class="table table-bordered table-hover" style="width:100%;">
                            <thead>
                                <tr class="bg-warning">
                                    <th>...</th>
                                    <th>ID</th>
                                    <th>MASCOTA</th>
                                    <th>COnfiabilidad</th>
                                </tr>
                            </thead>
                            <tbody id="tbResult_Det"></tbody>
                        </table>
                    </div>
                </div>

            </div>
            <br />
            <div id="ResultRecognition"></div>
            <div id="ListImgRecognition"></div>

            <br />
        </div>
    </div>
